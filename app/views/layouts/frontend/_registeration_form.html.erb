<div id="quickview-register">
            <!-- Modal -->
            <div class="modal fade" id="register" tabindex="-1" role="dialog">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true"><i class="bi bi-x"></i></span></button>
                        </div>
                        <div class="modal-body">
                            <div class="form-pop-up-content ptb-60 pl-60 pr-60">
                                <div class="area-title text-center mb-43">
                                    <img src="images/logo/logo.png" alt="jobhere">
                                </div>
                                <%= form_for(resource, as: resource_name, url: user_registration_path) do |f| %>

                                    <div class="form-box box2">
                                        <%= f.text_field :name, autofocus: true, autocomplete: "name",placeholder: "Name", class:"mb-14" %>
                                        <%= f.text_field :phone, autofocus: true, autocomplete: "phone" ,placeholder: "Phone", class:"mb-14" %>
                                        <!-- <input type="text" name="firstname" placeholder="First Name" class="mb-14"> -->
                                    </div>
                                    <div class="form-box">
                                        <%= f.email_field :email, autofocus: true, autocomplete: "email",  placeholder: "email", class:"mb-14" %>
                                        <%= f.text_field :password, autofocus: true, autocomplete: "password", placeholder: "password", class:"mb-14" %>
                                        <!-- <input type="email" name="emailnew" placeholder="Email" class="mb-14">
                                        <input type="password" name="pass" placeholder="Password"> -->
                                    </div>
                                    <div class="form-box">
                                        <%= f.text_field :password_confirmation, autofocus: true, autocomplete: "password", placeholder: "Confirm password", class:"mb-14" %>
                                    </div>
                                    <div class="fix ptb-30">
                                        <span class="remember pull-left"><input class="p-0 pull-left" type="checkbox">Remember Me</span>
                                    </div>
                                    <div class="text-center">
                                        <button type="submit" class="text-uppercase">Submit</button>
                                    </div>
                                <% end %>

                            </div>
                        </div>  
                    </div>  
                </div>
            </div>
        </div>